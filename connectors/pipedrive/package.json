{
  "name": "@runmorph/connector-pipedrive",
  "author": "HenriChabrand",
  "version": "0.0.9",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "morph build",
    "dev": "morph develop",
    "clean": "rm -rf ./dist",
    "validate": "run-p validate:*",
    "validate:format": "prettier --list-different \"**/*.{ts,tsx,md}\" --ignore-path ../../.gitignore",
    "validate:lint": "eslint --max-warnings=0 src/",
    "validate:typecheck": "tsc --noEmit",
    "fix": "run-p fix:*",
    "fix:lint": "eslint --fix src/",
    "fix:format": "prettier --write \"**/*.{ts,tsx,md}\" --ignore-path ../../.gitignore",
    "prepublishOnly": "yarn run build",
    "_publish": "yarn run build && yarn publish --access public"
  },
  "devDependencies": {
    "@runmorph/cdk": "^0.0.16",
    "@runmorph/resource-models": "^0.0.2"
  },
  "peerDependencies": {
    "@runmorph/cdk": "^0.0.16",
    "@runmorph/resource-models": "^0.0.2"
  }
}
